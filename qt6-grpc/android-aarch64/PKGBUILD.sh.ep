% layout 'android-qt6-module';
\
_qtver=6.10.0
pkgver=${_qtver/-/}
pkgrel=1
arch=(any)
url='https://www.qt.io'
license=(GPL3 LGPL3 FDL custom)
pkgdesc='Provides the modules QtGrpc and QtProtobuf (android)'
depends=(<%== qt6deps qw(base) %> "android-${_android_arch}-protobuf")
optdepends=(<%== qt6optdeps declarative => 'QML bindings' %>)
makedepends=('android-cmake<%== $static_suffix %>' "android-${_android_arch}-grpc" 'protobuf' 'qt6-grpc' 'qt6-declarative' 'qt6-shadertools' 'ninja')
