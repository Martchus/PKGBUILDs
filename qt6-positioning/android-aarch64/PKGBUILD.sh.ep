% layout 'android-qt6-module';
\
_qtver=6.10.0
pkgver=${_qtver/-/}
pkgrel=1
arch=(any)
url='https://www.qt.io'
license=(GPL-3.0-only
         LGPL-3.0-only
         LicenseRef-Qt-Commercial
         Qt-GPL-exception-1.0)
pkgdesc='Provides access to position, satellite and area monitoring classes (android)'
depends=(<%== qt6deps qw(base) %>)
optdepends=(<%== qt6optdeps serialport => 'NMEA plugin', declarative => 'QML bindings' %>)
makedepends=('android-cmake<%== $static_suffix %>' <%== qt6deps qw(serialport declarative) %> 'qt6-serialbus' 'qt6-declarative' 'ninja')
